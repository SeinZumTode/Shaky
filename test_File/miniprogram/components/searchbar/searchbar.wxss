/**searchbar.wxss**/
/* 搜索框 */
.searchbar {
  height: 88rpx;
  padding: 50rpx 30rpx;
  display: flex;
  background-color:#696969;
}
.content {
  height: 88rpx;
  padding: 0 18rpx;
  background: rgba(255,255,255,1);
  border-radius: 45rpx;
  border: 2rpx solid rgba(220,222,226,1);
  flex: 1;
  display: flex;
  align-items: center;
}
.search-icon {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: -4rpx;
}
.input {
  flex: 1;
  margin: 0 20rpx;
}
.delete-icon {
  width: 50rpx;
  height: 50rpx;
}

